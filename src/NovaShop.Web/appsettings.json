{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CatalogConnectionString": "Server=.\\SQL2019;Database=NovaShop_DB;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "CatalogCdnBaseUri": "nova-shop-cdn.mehdimst.com",
  "CatalogPictureBaseUri": "http://nova-shop-cdn.mehdimst.com/catalog/[0]",
  "CatalogGalleryPictureBaseUri": "http://nova-shop-cdn.mehdimst.com/catalog/gallery/[0]",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
